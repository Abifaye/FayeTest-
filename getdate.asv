function [date] = getdate(pwd);
%created by Faye 2210113
% inputs to the current folder, finds the name of the file, convert it to a
% string, and finally sparse it so
fileDir = dir('*.mat');
arrayFileName = struct2cell(fileDir);
strFileName =  string(arrayFileName);
delimiters = ".mat";
date = strtok(strFileName(1),delimiters);
end
